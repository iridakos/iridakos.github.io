layout: null
---
[{% for post in site.posts %}
  { "title": "{{ post.title | json }}",
    "description": "{% if post.description == null %}{{ post.preview | json }}{% else %}{{ post.description | json }}{% endif %}",
    "tags": "{{ post.search_tags }}",
    "categories": "{{ post.category }}",
    "url": "{{ post.url | prepend: site.baseurl | prepend: site.url }}"}{% if forloop.last == false %},{% endif %}{% endfor %}]
