---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | append: ' - cat photos' | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <image>
      <url>{{ site.url }}{{ site.baseurl }}/assets/images/site.png</url>
      <link>{{ site.url }}{{ site.baseurl }}/</link>
      <title>{{ site.title | xml_escape }}</title>
    </image>
    <atom:link href="{{ "/feeds/cat-photos.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>

    {% assign cats = "" | split:"/" %}
    {% for cat in site.cat_photos %}
      {% if cat.is_cat %}
        {% assign cats = cats | push: cat %}
      {% endif %}
    {% endfor %}

    {% for cat in cats limit:30 %}
      <item>
        <title>{{ cat.title | xml_escape }} / {{ cat.subtitle | xml_escape }}</title>
        <description><a href="{{ cat.url | prepend: site.baseurl | prepend: site.url }}"><img src="{{ cat.image | prepend: site.baseurl | prepend: site.url }}" /></a></description>
        <pubDate>{{ cat.date | date_to_rfc822 }}</pubDate>
        <link>{{ cat.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ cat.url | prepend: site.baseurl | prepend: site.url }}</guid>
        <category>cats</category>
      </item>
    {% endfor %}
  </channel>
</rss>
